{
    "manifest_version": 3,
    "name": "Simple Site Blocker",
    "version": "1.0.0",
    "content_scripts":[{
        "matches": ["<all_urls>"],
        "js":["popup.js"]
        } 
    ],
    "icons": {
        "48" : "calendario.png"
    },
    "action" :{
        "default_popup" :"index.html"
    }

}

